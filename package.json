{
	"name": "summa",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "npx panda codegen --config src/config/panda.config.ts && vite --config src/config/vite.config.ts",
		"build": "npx panda codegen --config src/config/panda.config.ts && tsc -b && vite build --config src/config/vite.config.ts",
		"lint": "eslint . --config src/config/eslint.config.js",
		"preview": "vite preview --config src/config/vite.config.ts",
		"format": "prettier --write --config src/config/.prettierrc '**/*.{ts,tsx,json,css,md}'",
		"test": "vitest --config src/config/vitest.config.ts",
		"test:run": "vitest run --config src/config/vitest.config.ts",
		"test:coverage": "vitest run --coverage --config src/config/vitest.config.ts"
	},
	"dependencies": {
		"react": "^19.1.0",
		"react-dom": "^19.1.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.20.0",
		"@pandacss/dev": "^0.53.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@types/react": "^19.1.0",
		"@types/react-dom": "^19.1.0",
		"@vitejs/plugin-react": "^4.3.4",
		"@vitest/coverage-v8": "^3.0.5",
		"eslint": "^9.20.0",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-react-refresh": "^0.4.18",
		"globals": "^15.15.0",
		"jsdom": "^26.0.0",
		"prettier": "^3.5.1",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.24.0",
		"vite": "^6.1.0",
		"vitest": "^3.0.5"
	}
}
