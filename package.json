{
	"name": "summa",
	"private": true,
	"version": "0.6.0",
	"type": "module",
	"scripts": {
		"dev": "npx panda codegen --config src/config/panda.config.mjs && vite --config src/config/vite.config.ts --host",
		"dev:local": "vite --config src/config/vite.config.ts",
		"build": "npx panda codegen --config src/config/panda.config.mjs && vite build --config src/config/vite.config.ts",
		"typecheck": "tsc --noEmit -p src/config/tsconfig.json",
		"lint": "eslint . --config src/config/eslint.config.js",
		"preview": "vite preview --config src/config/vite.config.ts",
		"format": "prettier --write --config src/config/.prettierrc '**/*.{ts,tsx,json,css,md}'",
		"prettier": "prettier --write --config src/config/.prettierrc '**/*.{ts,tsx,json,css,md}'",
		"test": "vitest --config src/config/vitest.config.ts",
		"test:run": "vitest run --config src/config/vitest.config.ts",
		"test:watch": "vitest watch --config src/config/vitest.config.ts",
		"test:coverage": "vitest run --coverage --config src/config/vitest.config.ts",
		"test:e2e": "playwright test --config src/config/playwright.config.js",
		"test:e2e:ui": "playwright test --config src/config/playwright.config.js --ui",
		"test:e2e:headed": "playwright test --config src/config/playwright.config.js --headed",
		"test:e2e:errors": "rm -f e2e-output.log && playwright test --config src/config/playwright.config.js 2>&1 | tee /dev/tty | grep -A 30 'Error:' > e2e-output.log || true",
		"test:e2e:codegen": "playwright codegen http://localhost:8080/Summa/"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"react": "^19.1.0",
		"react-dom": "^19.1.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.20.0",
		"@pandacss/dev": "^1.4.0",
		"@playwright/test": "^1.58.2",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^25.3.0",
		"@types/react": "^19.1.0",
		"@types/react-dom": "^19.1.0",
		"@vitejs/plugin-react": "^4.3.4",
		"@vitest/coverage-v8": "^3.0.5",
		"eslint": "^9.20.0",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-react-refresh": "^0.4.18",
		"globals": "^15.15.0",
		"jsdom": "^26.0.0",
		"prettier": "^3.5.1",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.24.0",
		"vite": "^6.1.0",
		"vitest": "^3.0.5"
	}
}
